{
  "functions": {
    "api/index.ts": {
      "maxDuration": 30
    }
  },
  "buildCommand": "node vercel-build-simple.js",
  "outputDirectory": "dist/public",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}