# Environment Variables Template for PQC Scanner
# Copy this file to .env and fill in your values

# Database Configuration
DATABASE_URL=postgresql://user:password@host:5432/database
PGHOST=localhost
PGPORT=5432
PGUSER=postgres
PGPASSWORD=your_password
PGDATABASE=pqc_scanner

# Application Configuration
NODE_ENV=development
PORT=5000
SESSION_SECRET=your_super_secure_session_secret_change_this

# Optional: External API Keys
# GitHub Integration (for repository scanning)
# GITHUB_TOKEN=ghp_your_github_personal_access_token

# SonarQube Integration  
# SONARQUBE_URL=https://your-sonarqube-instance.com
# SONARQUBE_TOKEN=your_sonarqube_token

# Jenkins Integration
# JENKINS_URL=https://your-jenkins-instance.com
# JENKINS_USER=your_jenkins_user
# JENKINS_TOKEN=your_jenkins_api_token

# AWS Configuration (for AWS deployment)
# AWS_ACCESS_KEY_ID=your_aws_access_key
# AWS_SECRET_ACCESS_KEY=your_aws_secret_key
# AWS_REGION=us-east-1

# Vercel/Production Configuration
# These are automatically set by the platform
# VERCEL_URL=your-app.vercel.app
# REPLIT_DOMAINS=your-app.replit.app